extends layout

block content
	script.
		$(document).ready(function() {
			if(!window.wellnessMain)
			{
				window.wellnessMain = new WELLNESS.CLIENT.Main();
			}
			window.wellnessMain.addEvents('home');
		});

	div.container
		div.row.row-no-top
			div.content.dash
				div.col-xs-3.col-sm-3.col-md-3.col-lg-3
				div.col-xs-6.col-sm-6.col-md-6.col-lg-6
					div.pageInstruct Select Dashboard
					div.arrowWhiteDown
				div.col-xs-3.col-sm-3.col-md-3.col-lg-3
