							p Operator
				div.col-xs-6.col-sm-6.col-md-6.col-lg-6.dashBtnCont
					a(href="#" class ='dashA' id="playerdash")
						div.dashBtn
							p Player

	div.popOverlay(style='display:none;')#traineepop
		div.dobPopUp
			div.playerCircle
				div.playerImage.dier
			div.dobInstruct Login
			label Username 
			input(type="text", name="user")#traineeuser
			label Password
			input(type="password", name="pass")#traineepass
			a(href='#' id='traineelogin')
